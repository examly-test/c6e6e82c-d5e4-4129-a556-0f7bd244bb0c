        <sonar.organization>neojarvis-testing</sonar.organization>
        <sonar.host.url>https://sonarcloud.io</sonar.host.url>
        <sonar.sources>angularapp/src,springapp/src,Testing-with-Selenium-TestNg/src/main</sonar.sources>
        <sonar.tests>karma,puppeteer,junit,springapp/src/test,Testing-with-Selenium-TestNg/src/test</sonar.tests>
